T = int(input())
# 여러개의 테스트 케이스가 주어지므로, 각각을 처리합니다.
for test_case in range(1, T + 1):
    # ///////////////////////////////////////////////////////////////////////////////////
    lst = list(map(int, input().split()))
	
    lst.sort()
    lst.pop(0)
    lst.pop()
    
    avg = round(sum(lst)/len(lst))
    
    print("#{} {}".format(test_case,avg))
