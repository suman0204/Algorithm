T = int(input())
# 여러개의 테스트 케이스가 주어지므로, 각각을 처리합니다.
for test_case in range(1, T + 1):
    # ///////////////////////////////////////////////////////////////////////////////////
    A, B = map(int, input().split())

    result = A + B

    if result ==24:
        result = 0
    elif result > 24:
        result = result-24
    
    print(("#{} {}".format(test_case,result)))
