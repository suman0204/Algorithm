T = int(input())
# 여러개의 테스트 케이스가 주어지므로, 각각을 처리합니다.
for tc in range(1, T + 1):
    # ///////////////////////////////////////////////////////////////////////////////////
    a = input()
    
    for i in range(1,10):
        if a[0:i] == a[i:i*2]:
            print("#{} {}".format(tc, i))
            break
