def solution(n, lost, reserve):
    answer = 0
    set_lost = set(lost) - set(reserve)
    set_res = set(reserve) - set(lost)
    
    for i in set_res:
        if i-1 in set_lost:
            set_lost.remove(i-1)
        elif i+1 in set_lost:
            set_lost.remove(i+1)
    print(set_lost)
    
    answer = n - len(set_lost)
    
    return answer
